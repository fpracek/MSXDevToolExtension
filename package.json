{
  "name": "msxdevtool-extension",
  "displayName": "MSXDevTool Extension",
  "description": "MSXDevTool Extension for Visual Studio Code",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "publisher": "Fausto Pracek",
  "activationEvents": [
    "onCommand:msxdevtool-extension.openWebview"
  ],
  "contributes": {
    "commands": [
      {
        "command": "msxdevtool-extension.openWebview",
        "title": "MSXDev tool"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/node": "16.x",
    "typescript": "^4.8.0"
  },
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/fpracek/MSXDevToolExtension.git"
  }
}
